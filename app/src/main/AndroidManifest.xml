<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/splashball"
        android:roundIcon="@drawable/ic_launcher_balon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChuteApp"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:theme="@style/SplashTheme"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StoreView"/>
        <activity android:name=".Equipos"/>
        <activity android:name=".Perfil"/>
        <activity android:name=".Crear"/>
        <activity android:name=".Maps"/>
        <activity android:name=".MisEquipos"/>
        <activity android:name=".Equipo"/>


    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

</manifest>